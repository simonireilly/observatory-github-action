{
  "name": "observatory-github-action",
  "description": "Observatory CLI runner for github actions",
  "version": "0.1.2",
  "author": "",
  "dependencies": {
    "@actions/core": "1.9.1",
    "@actions/exec": "1.1.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/jest": "28.1.6",
    "@types/node": "18.7.3",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.33.0",
    "@vercel/ncc": "0.34.0",
    "embedme": "1.22.0",
    "eslint": "8.22.0",
    "eslint-plugin-github": "4.3.7",
    "eslint-plugin-jest": "26.8.2",
    "husky": "^8.0.1",
    "jest": "28.1.3",
    "js-yaml": "4.1.0",
    "release-it": "15.3.0",
    "syncpack": "^8.2.4",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "license": "MIT",
  "main": "lib/main.js",
  "private": true,
  "repository": "simonireilly/observatory-github-action",
  "scripts": {
    "all": "npm run build && npm run package",
    "build": "tsc --build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "npm run lint --fix",
    "observatory-cli": "observatory",
    "package": "ncc build --minify --license LICENSE",
    "prepare": "husky install",
    "pub:beta": "release-it --preRelease=beta --no-npm.publish",
    "pub:latest": "release-it --no-npm.publish",
    "readme": "embedme README.md",
    "test": "jest unit",
    "test:integration": "jest --runInBand integration"
  }
}
